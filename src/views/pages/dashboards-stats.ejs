<!DOCTYPE html>
<html lang="en">
<head>
  <%- include ../layouts/layout-header.ejs %>

</head>
<body class="dashboard-body">
<div style="display:none" id="secret" username="<%=username%>"></div>
<div id="alert-container" style="position:absolute;top:100px;right:0"></div>
<div class="container" id="stats-container" >
  <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
    <img class="github" id="github" alt="Houman Hadian's project research tool on Github" src="/img/svg/github-corner-right.svg">
    <div class="navbar-header">
      <button class="instabtn" onclick="openNav('dashboard-sidepanel', '450px')">&#9776;</button>
      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="">
        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"
             viewBox="76.39 132.243 15.35 22" width="20" height="20" fill="#777">
          <defs>
            <path id="a"
                  d="M88.31 136.63c.29-.28.43-.63.43-1.05 0-.41-.14-.76-.43-1.04-.08-.09-.78-.78-.86-.87-.29-.28-.64-.43-1.05-.43-.41 0-.76.15-1.05.43-.75.75-6.78 6.77-7.53 7.53-.29.28-.43.63-.43 1.04 0 .42.14.76.43 1.05.75.75 6.78 6.77 7.53 7.53.29.28.64.42 1.05.42.41 0 .76-.14 1.05-.42.08-.09.78-.78.86-.87.29-.29.43-.63.43-1.04 0-.41-.14-.76-.43-1.05-.37-.38-2.24-2.25-5.61-5.61 3.37-3.37 5.24-5.24 5.61-5.62z" />
          </defs>
          <use xlink:href="#a" />
          <use xlink:href="#a" />
        </svg>
      </a>
      <a class="navbar-brand" href="/home?username=<%=username%>">Socialytics &raquo; stats</a>
    </div>
    <div class="navbar-collapse">
      <ul class="navbar-nav nav main-nav">
        <li><a id="home-link" href="/home?username=<%=username%>">Home</a></li>
        <li><a href="/dashboards/explore?username=<%=username%>">Data explore</a></li>
        <li><a href="/dashboards/engagement?username=<%=username%>">Engagement dashboard</a>
        </li>
      </ul>
    </div>
  </div>
</div>
<div  class="main-container">
  <div class="dash-container grid grid-split-columns">
    <div class="widget-wrapper">
      <div class="widget-title">
        Profiles A Followers
      </div>
      <div style="min-width: 100px;min-height:200px;" id="stats-hbar-a" class="widget-stage classa">
        <img class="holders" data-src="holder.js/100px120?theme=white">
      </div>
      <div class="widget-notes"></div>
    </div>

    <div class="widget-wrapper">
      <div class="widget-title">
        Profiles A followers metrics
      </div>
      <div id="profiles-counters-a" class="widget-stage classa counters-stage">
        <img class="holders" data-src="holder.js/100px120?theme=white">
      </div>
      <div style="margin-left:32px;margin-top:30px;" id="profileA-metrics-legend" class="widget-notes">
      </div>
    </div>

    <div class="widget-wrapper">
      <div class="widget-title">
        Profiles B Followers
      </div>
      <div style="min-width: 100px;min-height:200px;" id="stats-hbar-b" class="widget-stage classb">
        <img class="holders" data-src="holder.js/100px120?theme=white">
      </div>
      <div class="widget-notes"></div>
    </div>

    <div class="widget-wrapper">
      <div  class="widget-title">
        Profiles B followers metrics
      </div>
      <div id="profiles-counters-b" class="widget-stage classb counters-stage">
        <img class="holders" data-src="holder.js/100px120?theme=white">
      </div>
      <div style="margin-left:32px;margin-top:30px;" id="profileB-metrics-legend" class="widget-notes">
      </div>
    </div>
    <div class="grid-split">
      <div class="widget-wrapper">
        <div class="widget-title">
          Profiles A recent engagement trends
        </div>
        <div id="lines-seta" class="widget-stage classa">
          <img class="holders" data-src="holder.js/100px120?theme=white">
        </div>
        <div class="widget-notes"></div>
      </div>
      <div class="widget-wrapper">
        <div class="widget-title">
          Profiles B recent engagement trends
        </div>
        <div id="lines-setb" class="widget-stage classb">
          <img class="holders" data-src="holder.js/100px120?theme=white">
        </div>
        <div class="widget-notes"></div>
      </div>
      <div class="widget-wrapper">
        <div class="widget-title">
          Set A Heatmap calendar
        </div>
        <div id="heatA" class="widget-stage classa">
          <img class="holders" data-src="holder.js/100px120?theme=white">
        </div>
        <div class="widget-notes"></div>
      </div>
      <div class="widget-wrapper">
        <div class="widget-title">
          Set B Heatmap calendar
        </div>
        <div id="heatB" class="widget-stage classb">
          <img class="holders" data-src="holder.js/100px120?theme=white">
        </div>
        <div class="widget-notes"></div>
      </div>

      <div class="widget-wrapper">
        <div class="widget-title">
          Profiles geo charts A
        </div>
        <div  style="max-width:45vw;width:100%;height:200px;" id="geoA" class="widget-stage classa">
          <img class="holders" data-src="holder.js/100px120?theme=white">
        </div>
        <div class="widget-notes"></div>
      </div>
      <div class="widget-wrapper">
        <div class="widget-title">
          Profiles geo charts B
        </div>
        <div  style="max-width:45vw;width:100%;height:200px;" id="geoB" class="widget-stage classb">
          <img class="holders" data-src="holder.js/100px120?theme=white">
        </div>
        <div class="widget-notes"></div>
      </div>

      <div class="widget-wrapper">
        <div class="widget-title">
          Profiles pie charts A
        </div>
        <div  style="max-width:45vw" id="pie-seta" class="widget-stage tabs">
          <!-- Nav tabs -->
          <ul style="max-width:45vw" class="nav nav-tabs">
            <li class="nav-item">
              <a class="nav-link active" data-toggle="tab" href="#DoMA">DoM</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" data-toggle="tab" href="#DoWA">DoW</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" data-toggle="tab" href="#HoDA">HoD</a>
            </li>
          </ul>

          <!-- Tab panes -->
          <div style="max-width:45vw" class="tab-content">
            <div style="max-width:45vw" class="tab-pane active container widget-stage classa" id="DoMA">
              <img class="holders" data-src="holder.js/100px120?theme=white">
            </div>
            <div style="max-width:45vw" class="tab-pane container widget-stage classa" id="DoWA">
              <img class="holders" data-src="holder.js/100px120?theme=white">
            </div>
            <div style="max-width:45vw" class="tab-pane container widget-stage classa" id="HoDA">
              <img class="holders" data-src="holder.js/100px120?theme=white">
            </div>
          </div>
        </div>
        <div class="widget-notes"></div>
      </div>
      <div class="widget-wrapper">
        <div class="widget-title">
          Profiles pie charts B
        </div>
        <div style="max-width:45vw" id="pie-setb" class="widget-stage tabs">
          <!-- Nav tabs -->
          <ul style="max-width:45vw" class="nav nav-tabs">
            <li class="nav-item">
              <a class="nav-link active" data-toggle="tab" href="#DoMB">DoM</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" data-toggle="tab" href="#DoWB">DoW</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" data-toggle="tab" href="#HoDB">HoD</a>
            </li>
          </ul>

          <!-- Tab panes -->
          <div style="max-width:45vw" class="tab-content">
            <div style="max-width:45vw" class="tab-pane active container widget-stage classb" id="DoMB">
              <img class="holders" data-src="holder.js/100px120?theme=white">
            </div>
            <div style="max-width:45vw" class="tab-pane container widget-stage classb" id="DoWB">
              <img class="holders" data-src="holder.js/100px120?theme=white">
            </div>
            <div style="max-width:45vw" class="tab-pane container widget-stage classb" id="HoDB">
              <img class="holders" data-src="holder.js/100px120?theme=white">
            </div>
          </div>
        </div>
        <div class="widget-notes"></div>
      </div>


    </div>
    <div class="widget-wrapper">
      <div class="widget-title">
        Profiles maps A
      </div>
      <div class="widget-stage classa">
        <img class="holders" data-src="holder.js/100px120?theme=white">
      </div>
      <div class="widget-notes"></div>
    </div>
    <div class="widget-wrapper">
      <div class="widget-title">
        Set A Total metrics
      </div>
      <div id="total-metrics-a" class="widget-stage classa">
        <img class="holders" data-src="holder.js/100px120?theme=white">
      </div>
      <div class="widget-notes"></div>
    </div>
    <div class="widget-wrapper">
      <div class="widget-title">
        Profiles maps B
      </div>
      <div class="widget-stage classb">
        <img class="holders" data-src="holder.js/100px120?theme=white">
      </div>
      <div class="widget-notes"></div>
    </div>
    <div class="widget-wrapper">
      <div class="widget-title">
        Set B total metrics
      </div>
      <div id="total-metrics-b" class="widget-stage classb">
        <img class="holders" data-src="holder.js/100px120?theme=white">
      </div>
      <div class="widget-notes"></div>
    </div>
  </div>
</div>
<%- include ../pages/sidepanel-dashboards.ejs %>
<%- include ../layouts/layout-footer.ejs %>


<script>
  getEndpointData('/instagram/sets/data?', 'form-data', '', 'main', undefined);
  $("#noDataMsg").remove();
</script>
</body>
</html>

